{
  "name": "miproyecto-taller",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "react-scripts start",
    "json-server": "json-server ./src/data/home.json --port 4000",
    "json-server-catalog": "json-server ./src/data/catalog.json --port 4001",
    "dev": "concurrently \"npm start\" \"npm run json-server\" \"npm run json-server-catalog\" \"vite\"",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "publish": "docker build -t dgajardoi/grupo-6-frontend:entrega-10 .",
    "postpublish": "docker push dgajardoi/grupo-6-frontend:entrega-10"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.3.0",
    "axios": "^1.7.7",
    "bootstrap": "^5.3.3",
    "cors": "^2.8.5",
    "jwt-decode": "^4.0.0",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.6",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "react-icons": "^5.3.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@svgr/webpack": "^8.1.0",
    "@types/axios": "^0.9.36",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "concurrently": "^9.1.0",
    "eslint": "^9.9.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "json-server": "^1.0.0-beta.3",
    "react-scripts": "^5.0.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vite-plugin-svgr": "^4.3.0"
  }
}
